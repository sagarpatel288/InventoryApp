<?xml version="1.0" encoding="utf-8"?>
<resources>
    <!-- The app branding color for the app bar -->
    <color name="colorPrimary">@color/colorOrange</color>

    <!-- Darker variant for the status bar and contextual app bars -->
    <color name="colorPrimaryDark">@color/colorAccent</color>

    <!-- Accent color for UI controls like checkboxes and text fields -->
    <color name="colorAccent">@color/colorOrange</color>

    <!-- Control widget customization color : Image border, round button border-->
    <color name="colorOrange">#e57373</color>

    <!-- Control widget filled color. Eg Default circular image, round button-->
    <color name="colorWhite">#ffffff</color>

    <!-- All text color-->
    <color name="colorBlack">#000000</color>
</resources>
